{
    "name": "bitmicrosys/sharpsports-php",
    "description": "PHP Laravel SDK for SharpSports API",
    "version": "1.0.0",
    "keywords": ["sharpsports", "sports betting", "api", "laravel", "sdk"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Bitmicrosys\\SharpsportsPhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Prakash Timilsina"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bitmicrosys\\SharpsportsPhp\\SharpsportsServiceProvider"
            ],
            "aliases": {
                "SharpSports": "Bitmicrosys\\SharpsportsPhp\\Facades\\SharpSports"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
